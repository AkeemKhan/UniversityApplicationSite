@model IEnumerable<NAA.Data.University>

@{
    ViewBag.Title = "UniversityManager";
}

<h2>University Manager</h2>

<p>
    @Html.ActionLink("Add University", "AddUniversity", new {Controller = "NAAAdmin"})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UniversityName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UniversityName)
        </td>
    </tr>
}

</table>
